{
  "name": "my-awesome-hunter-backend",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node dist/server.js",
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project ./ && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:migrations": "./build-migrations.sh",
    "test": "NODE_ENV=test jest --passWithNoTests --no-cache --runInBand",
    "test:ci": "npm test -- --coverage",
    "eslint": "eslint . --fix",
    "migration:generate": "sequelize migration:generate",
    "migration:apply": "npm run build:migrations && sequelize db:migrate --config './compiled/config/database.js' --migrations-path './compiled/database/migrations'",
    "migration:apply:seed": "npm run build && node dist/database/import-seeds.js",
    "migration:undo": "npm run build:migrations && sequelize db:migrate:undo --config './compiled/config/database.js' --migrations-path './compiled/database/migrations'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatheusHAS/my-awesome-hunter-backend.git"
  },
  "author": "Matheus Azambuja <matheushenrique.ads@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MatheusHAS/my-awesome-hunter-backend/issues"
  },
  "homepage": "https://github.com/MatheusHAS/my-awesome-hunter-backend#readme",
  "dependencies": {
    "@types/validator": "^13.1.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lusca": "^1.6.1",
    "module-alias": "^2.2.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.0.0",
    "ts-node": "^9.1.1",
    "tscpaths": "0.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/lusca": "^1.6.2",
    "@types/node": "^14.14.25",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "faker": "^5.3.1",
    "git-commit-msg-linter": "^3.0.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
